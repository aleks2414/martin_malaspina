<!DOCTYPE html>
<html>
<head>
  <title>Pruebas</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script src="https://use.fontawesome.com/8e56b09395.js"></script>
<link href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet">
</head>
<body>

<% if alert %>
	<p class="alert"><%= alert %></p>
<% elsif notice %>
	<p class="notice"><%= notice %></p>
<% end %>


<ul class="nav navbar-nav navbar-right">
 <% if user_signed_in? %>
 <li> <a href="<%= destroy_user_session_path %>" data-method='delete'>
 Salir
 </a></li>
 <% else %>
 <li><a href="<%= new_user_session_path %>"> Login </a></li>
 <li><a href="<%= new_user_registration_path %>"> Registro </a></li>
 <% end %>
</ul>

<%= yield %>

</body>
</html>
