<p id="notice"><%= notice %></p>

<h1>Listing Tests</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Colegio</th>
      <th>Nivel</th>
      <th>Seccion</th>
      <th>Edad</th>
      <th>Ic</th>
      <th>Icm</th>
      <th>Pi num</th>
      <th>Pi comp</th>
      <th>Pi cal</th>
      <th>Pi conc</th>
      <th>Pf conv</th>
      <th>Pf hecho</th>
      <th>Pf calc</th>
      <th>Pf conce</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td><%= test.nombre %></td>
        <td><%= test.colegio %></td>
        <td><%= test.nivel %></td>
        <td><%= test.seccion %></td>
        <td><%= test.edad %></td>
        <td><%= test.ic %></td>
        <td><%= test.icm %></td>
        <td><%= test.pi_num %></td>
        <td><%= test.pi_comp %></td>
        <td><%= test.pi_cal %></td>
        <td><%= test.pi_conc %></td>
        <td><%= test.pf_conv %></td>
        <td><%= test.pf_hecho %></td>
        <td><%= test.pf_calc %></td>
        <td><%= test.pf_conce %></td>
        <td><%= link_to 'Show', test %></td>
        <td><%= link_to 'Edit', edit_test_path(test) %></td>
        <td><%= link_to 'Destroy', test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Test', new_test_path %>
